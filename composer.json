{
    "name": "sofyan/wcsdm",
    "description": "WooCommerce shipping rates calculator that allows you to easily offer shipping rates based on the distance that calculated using Google Maps Distance Matrix Service API.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Sofyan Sitorus",
            "email": "sofyansitorus@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "woocommerce/woocommerce": "dev-master",
        "phpcompatibility/php-compatibility": "*",
        "phpcompatibility/phpcompatibility-wp": "*",
        "wp-coding-standards/wpcs": "^2.1",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "sirbrillig/phpcs-variable-analysis": "^2.7",
        "phpunit/phpunit": "^5"
    },
    "autoload-dev": {
        "classmap": [
            "vendor/woocommerce/woocommerce/tests/framework"
        ]
    },
    "extra": {
        "installer-paths": {
            "vendor/woocommerce/woocommerce": [
                "woocommerce/woocommerce"
            ],
            "vendor/woocommerce/woocommerce-blocks": [
                "woocommerce/woocommerce-blocks"
            ],
            "vendor/woocommerce/woocommerce-rest-api": [
                "woocommerce/woocommerce-rest-api"
            ]
        }
    }
}
